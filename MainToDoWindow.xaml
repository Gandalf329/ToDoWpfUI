<Window x:Class="WpfUI1.MainToDoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfUI1"
        mc:Ignorable="d"
        Title="Register" Height="650" Width="800"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" >
    <Grid>
        <Label Content="ToDo Today" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="36" FontWeight="Bold" Foreground="#FFFF9523"/>
        <Frame x:Name="Main" Margin="125,62,50,17" Width="625"/>
        <ListBox Margin="0,67,675,0">
            <ListBox.Items>
                <ListBoxItem Background="#BFFF9523" BorderBrush="#BFFF9523">Today</ListBoxItem>
                <ListBoxItem Background="#BFFF9523" BorderBrush="#BFFF9523">Favorites</ListBoxItem>
                <ListBoxItem Background="#BFFF9523" BorderBrush="#BFFF9523">Project 1</ListBoxItem>
            </ListBox.Items>
        </ListBox>

    </Grid>
</Window>
